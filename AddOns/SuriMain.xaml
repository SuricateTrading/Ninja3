            <TabItem Header="Inhalte" Padding="10,5">
                <ScrollViewer>
                    <Grid VerticalAlignment="Top">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="9*"/>
                            <ColumnDefinition Width="50*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Border Grid.Column="0" Grid.Row="0" BorderBrush="#FF0096FF" BorderThickness="1">
                            <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                Indikatoren
                            </TextBlock>
                        </Border>
                        <Grid Grid.Column="1" Grid.Row="0" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="14*" />
                                <ColumnDefinition Width="50*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                CoT 1
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="0" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Der CoT 1 Indikator nach Regelwerk.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="1" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                CoT 2
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="1" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Der CoT 2 Indikator nach Regelwerk.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="2" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                CoT-Daten
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="2" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                Hiermit kannst Du Dir andere CoT Daten anzeigen, z.B. das Open Interest.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="3" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                SMA
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="3" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                Der SMA ist grün, wenn er im Vergleich zum Vortag steigend ist, oder rot, wenn er fallend ist.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="4" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                Oszillator
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="4" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Ein Oszillator, der einen Wert in Bezug zu den letzten 125 Bars berechnet.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="5" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                Com Short / OI in %
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="5" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Der Anteil der Commercials Short am Open Interest.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="6" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                Bargröße
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="6" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                Die Bargröße im Vergleich zu den letzten 125 Tagen.<LineBreak/>
                                Falls diese Bar die größte in diesem Zeitraum ist, gilt sie als Megabar und wird gelb markiert.<LineBreak/>
                                Gaps werden mit einberechnet.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="7" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                    Volumen
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="7" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Das Volumen im Vergleich zu den letzten 125 Tagen.<LineBreak/>
                                    Falls es das höchste Volumen in diesem Zeitraum ist, gilt sie als Megavolumen und wird gelb markiert.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="8" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                    Mega
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="8" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Kombiniert die beiden Indikatoren Volumen und Bargröße und zeigt an, um wie viel Prozent ein Megabar oder Megavolumen erreicht wurde.<LineBreak/>
                                    Wenn beispielsweise Volumen auf 40% und Bargröße auf 60% ist, dann wird 60% angezeigt. 100% ist eine Megabar oder Megavolumen.
                                </TextBlock>
                            </Border>

                            <Border Grid.Column="0" Grid.Row="9" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                    Volume Profile Gross
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="9" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">

                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="10" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                    Volume Profile Intraday
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="10" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Dieser Indikator benötigt Tickdaten!
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="11" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                Toolbar
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="11" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                Die Toolbar lädt ueber dem Chart ein Menü, mit dem man Indikatoren ein- und ausschalten kann.<LineBreak/>
                                Der Indikator muss allerdings zuvor im Indikatormenü geladen sein.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="12" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                Zeichenwerkzeuge
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="12" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                Lädt innerhalb des Charts ein bewegliches, kleines Menue, mit dem man schneller auf Zeichenwerkzeuge zugreifen kann.
                                </TextBlock>
                            </Border>
                        </Grid>


                        <Border Grid.Column="0" Grid.Row="1" BorderBrush="#FF0096FF" BorderThickness="1">
                            <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                Zeichenwerkzeuge
                            </TextBlock>
                        </Border>
                        <Grid Grid.Column="1" Grid.Row="1" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="14*" />
                                <ColumnDefinition Width="50*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                    Suri Rechteck
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="0" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Zeichnet ein Rechteck um Bars herum mit einigen zusätzlichen Informationen wie die Anzahl an Ticks oder die Dauer in Tagen oder Bars.
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="0" Grid.Row="1" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                    Barinfo
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="1" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Zeichnet Informationen zu einer einzelnen Bar an, wie die Anzahl an Ticks der Bar, des Körpers oder der Dochte.
                                </TextBlock>
                            </Border>
                        </Grid>


                        <Border Grid.Column="0" Grid.Row="2" BorderBrush="#FF0096FF" BorderThickness="1">
                            <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                Chart Style
                            </TextBlock>
                        </Border>
                        <Grid Grid.Column="1" Grid.Row="2" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="14*" />
                                <ColumnDefinition Width="50*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Center">
                                Candlestick mit Gaps
                                </TextBlock>
                            </Border>
                            <Border Grid.Column="1" Grid.Row="0" BorderBrush="#FF0096FF" BorderThickness="1">
                                <TextBlock Foreground ="LightGray" FontSize="16" TextWrapping="WrapWithOverflow" TextAlignment="Center" VerticalAlignment="Top" Margin="10">
                                    Für unser Regelwerk spielen Gaps eine wichtige Rolle. Für uns gehört das Gap zu einer Bar dazu.<LineBreak/>
                                    Mit diesem Chart Style kannst Du Dir das Gap zu einer Bar anzeigen lassen.
                                </TextBlock>
                            </Border>
                        </Grid>
                    </Grid>
                </ScrollViewer>
